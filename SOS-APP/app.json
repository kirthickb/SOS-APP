{
  "expo": {
    "name": "sos-app",
    "slug": "sos-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sosapp",
    "userInterfaceStyle": "automatic",

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nishanth21.sosapp",
      "googleMapsApiKey": "AIzaSyAlM6ydnd7-cE7Y9YSqLoF7G4iHWk0Sl8k",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to send emergency SOS alerts with your current coordinates.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to track ambulance and provide real-time navigation."
      }
    },

    "android": {
      "package": "com.nishanth21.sosapp",
      "googleMapsApiKey": "AIzaSyAlM6ydnd7-cE7Y9YSqLoF7G4iHWk0Sl8k",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      }
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],

    "experiments": {
      "reactCompiler": true,
      "newArchEnabled": true
    },

    "extra": {
      "eas": {
        "projectId": "fdb821e4-487c-40ea-8fa9-29e8af51d30a"
      }
    }
  }
}
